    "@aragon/test-helpers": "^1.1.0",
    "coveralls": "3.0.2",
    "cross-env": "^5.2.0",
    "eth-gas-reporter": "^0.1.5",
    "solidity-coverage": "^0.6.2",
    "solidity-sha3": "^0.4.1",
    "solium": "^1.2.3",
    "truffle": "5.0.1",
    "truffle-extract": "^1.2.1"
